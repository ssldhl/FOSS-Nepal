<% provide(:title, 'Edit Meeting Outcome') %>
<% meta :description => "Edit Meeting Outcome",
        :keywords => "Edit Meeting Outcome, FOSS Nepal" %>
<header class="jumbotron subhead" id="overview">
  <div class="container">
    <h1>Edit Result</h1>
    <p class="lead">Update the outcomes of the meeting.</p>
  </div>
</header>
<h1 class="text-center">Update Meeting Outcome</h1>
<div class="container">
<div class="row">
  <div class="span6 offset3">
    <%= form_for [@current_meeting, @outcome], :url => meeting_outcome_path(@current_meeting) do |f| %>
    	<%= render 'shared/outcomeerror_messages'%>

      <%= f.label :result %>
      <%= f.text_area :result, cols: 60, rows: 15, placeholder: "Add the html tags to format texts.", required: "" %>

      <%= f.submit "Update outcome", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
</div>